  <div class="bookings">
    <h2>My Bookings</h2>
            <form method="post">
              <div class="profile-head">
                            <ul class="nav nav-tabs" id="myTab" role="tablist">
                                <li class="nav-item">
                                    <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home" aria-selected="true">Open Bookings</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">Past Bookings</a>
                                </li>
                            </ul>
              </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="tab-content profile-tab" id="myTabContent">
                            <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
                              <div class="card-product">
                                <img src="https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/skateboard.jpg" />
                                <div class="card-product-infos">
                                  <h2></h2>
                                  <p></p>
                                </div>
                              </div>
                            </div>
                            <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
                              <div class="card-product">
                                <img src="https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/skateboard.jpg" />
                                <div class="card-product-infos">
                                  <h2><%= @flat.title %></h2>
                                  <p><%= @flat.description %></p>
                                </div>
                              </div>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
  <ul>
    <% current_user.bookings.each do |booking| %>
      <li>
        <%= booking.start_date %>
      </li>
    <% end %>
  </ul>
